{
  "name": "Tab Custom Multiname",
  "version": "1.0",
  "description": "Allows to have your prefererred tab name",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js",
      "install.js",
      "hot-reload.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "options_page": "options/index.html"
}
